                _ => Ok(Response::builder().status(StatusCode::NOT_FOUND)
                        .body(Full::new(Bytes::new()))
                        .expect("Unable to create Not Found response"))
            }
        }
        Box::pin(run(
            self.api_impl.clone(),
            req,
{{#apiUsesMultipartFormData}}
            self.multipart_form_size_limit,
{{/apiUsesMultipartFormData}}
        ))
    }
}
